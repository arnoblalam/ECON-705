Cell["Exercise 12", "Section",
 CellChangeTimes->{{3.6579635456417823`*^9, 3.657963547662117*^9}}],

Cell[TextData[{
 "A ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", 
    RowBox[{"-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", 
    RowBox[{"-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " A = I (def of inverse)\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["I", "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = I (identity matrix is symmetric)\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"A", " ", 
      SuperscriptBox["A", 
       RowBox[{"-", "1"}]]}], ")"}], "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["AA", 
    RowBox[{"-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " (identity matrix is symmetric)\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"A", " ", 
      SuperscriptBox["A", 
       RowBox[{"-", "1"}]]}], ")"}], "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     SuperscriptBox["A", 
      RowBox[{"-", "1"}]], ")"}], "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " (law of product of transpose)\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", 
    RowBox[{"-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " A = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     SuperscriptBox["A", 
      RowBox[{"-", "1"}]], ")"}], "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " (using ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["A", 
     RowBox[{"-", "1"}]], "A"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["AA", 
    RowBox[{"-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=I)\n",
 Cell[BoxData[
