  FormBox[
   FractionBox["100", 
    SuperscriptBox["e", 
     RowBox[{"0.1", "*", "2"}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 172.36\nc) PV = 100 + ",
 Cell[BoxData[
  FormBox[
   FractionBox["95", 
    SuperscriptBox["e", 
     RowBox[{"0.1", "*", "2"}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 177.78\n\nOption C has the highest PV\n\n5.12\na) ",
 Cell[BoxData[
  FormBox[
   FractionBox["500", 
    SuperscriptBox["e", 
     RowBox[{".1", "*", "1"}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " + ",
 Cell[BoxData[
  FormBox[
   FractionBox["500", 
    SuperscriptBox["e", 
     RowBox[{".1", "*", "2"}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " +",
 Cell[BoxData[
  FormBox[
   FractionBox["500", 
    SuperscriptBox["e", 
     RowBox[{".1", "*", "3"}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " +",
 Cell[BoxData[
  FormBox[
   FractionBox["500", 
    SuperscriptBox["e", 
     RowBox[{".1", "*", "4"}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " +",
 Cell[BoxData[
  FormBox[
   FractionBox["500", 
    SuperscriptBox["e", 
     RowBox[{".1", "*", "5"}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 1870.62\nb) 500 ",
 Cell[BoxData[
  FormBox[
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"n", " ", "=", " ", "1"}], "\[Infinity]"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", 
    SuperscriptBox["e", 
     RowBox[{".1", "n"}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 500 ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", 
    RowBox[{
     SuperscriptBox["e", 
      FractionBox[
       RowBox[{"-", "1"}], "10"]], "-", " ", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=4754.17\n5.13\nB(t) = 2 ",
 Cell[BoxData[
  FormBox[
   SqrtBox["t"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nln B(t)  = ln 2 ",
 Cell[BoxData[
  FormBox[
   SqrtBox["t"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"B", "'"}], 
    RowBox[{"(", "t", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ln 2\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"B", "'"}], 
     RowBox[{"(", "t", ")"}]}], 
    RowBox[{"B", "(", "t", ")"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"ln", " ", "2"}], 
    RowBox[{"2", 
     SqrtBox["t"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 0.05\nt = 48.05\n\n5.14\nln V(t) = K + ",
 Cell[BoxData[
  FormBox[
   SqrtBox["t"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nV\[CloseCurlyQuote]/V = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   SqrtBox["t"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = r\nt  = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", 
    RowBox[{"4", 
     SuperscriptBox["r", "2"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\n5.15\nln V(t) = ln 2000 + ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["t", 
    RowBox[{"1", "/", "4"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"V", "'"}], "V"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "= ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "4"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["t", 
    RowBox[{
     RowBox[{"-", "3"}], "/", "4"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = r\nt = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"4", "r"}], ")"}], 
    RowBox[{
     RowBox[{"-", "4"}], "/", "3"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nr = 0.1  --> t = 3.39\n\n5.16\na) ln f(x) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"1", "/", "2"}], 
    RowBox[{"ln", "(", 
     RowBox[{
      SuperscriptBox["x", "2"], "+", "1"}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " - ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{"ln", "(", 
    SuperscriptBox["x", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "+4)\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"f", "'"}], "f"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox["x", 
    RowBox[{
     SuperscriptBox["x", "2"], "+", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " - ",
 Cell[BoxData[
  FormBox[
   FractionBox["x", 
    RowBox[{
     SuperscriptBox["x", "2"], " ", "+", " ", "4"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nf\[CloseCurlyQuote](x) = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"3", "x"}], 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["x", "2"], "+", "1"}], ")"}], 
      RowBox[{"1", "/", "2"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["x", "2"], "+", "4"}], ")"}], 
      RowBox[{"3", "/", "2"}]]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nb) ln f(x) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", 
    SuperscriptBox["x", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ln x\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"f", "'"}], "f"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 4x ln x 1 2x = 2x(1 + ln ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ")\nf\[CloseCurlyQuote](x) = 2x(1 + ln ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "2"], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     SuperscriptBox["x", "2"], ")"}], 
    SuperscriptBox["x", "2"]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\n5.17\nh(x) = f(x)g(x)\nln h = ln f + ln g\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"h", "'"}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"f", "'"}], "f"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " + ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"g", "'"}], "g"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nx ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"h", "'"}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "(", 
    RowBox[{
     FractionBox[
      RowBox[{"f", "'"}], "f"], "+", 
     FractionBox[
