Cell[TextData[{
 "a) The diemensions of ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is K\[Times]R.\nb) ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "A has diemnsions K\[Times]K. ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["A", "T"], "A"}], ")"}], 
    RowBox[{"-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " also has diemnsions K\[Times]K. ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"A", "(", 
     RowBox[{
      SuperscriptBox["A", "T"], "A"}], ")"}], 
    RowBox[{"-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " has diemnsions R\[Times]K. ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"A", "(", 
     RowBox[{
      SuperscriptBox["A", "T"], "A"}], ")"}], 
    RowBox[{"-", "1"}]], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " has diemnsions K\[Times]K.  Therefore M = I \[Minus] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["A", "T"], "A"}], ")"}], 
     RowBox[{"\[Minus]", "1"}]], 
    SuperscriptBox["A", "T"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " has diemnsions K\[Times]K"
}], "Text",
 CellChangeTimes->{{3.657961746328537*^9, 3.6579618057541294`*^9}, {
  3.657961842878831*^9, 3.6579620050348525`*^9}, {3.6579620445329876`*^9, 
  3.6579621499628687`*^9}, {3.6579625053947015`*^9, 3.65796250661347*^9}, {
  3.6579625434250045`*^9, 3.6579625454123397`*^9}}]
