Cell[TextData[{
 "5.11\na) PV = ",
 Cell[BoxData[
  FormBox[
   FractionBox["215", 
    SuperscriptBox["e", 
     RowBox[{"0.1", "*", "2"}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
