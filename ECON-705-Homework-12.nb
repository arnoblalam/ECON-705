Cell[TextData[{
 "f(x) = 3x + 5\n\[CapitalDelta]f(x)=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"f", "(", 
      RowBox[{
       SubscriptBox["x", "0"], "+", "h"}], ")"}], " ", "-", " ", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], 
    RowBox[{
     SubscriptBox["x", "0"], "+", "h", " ", "-", " ", 
     SubscriptBox["x", "0"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"3", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", "0"], "+", " ", "h"}], ")"}]}], " ", "+", " ", "5", 
     " ", "-", " ", 
     RowBox[{"[", 
      RowBox[{
       RowBox[{"3", 
        RowBox[{"(", 
         SubscriptBox["x", "0"], ")"}]}], " ", "+", " ", "5"}], "]"}]}], "h"],
    TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"3", 
      SubscriptBox["x", "0"]}], "+", 
     RowBox[{"3", "h"}], " ", "+", "5", " ", "-", 
     RowBox[{"3", 
      SubscriptBox["x", "0"]}], "-", " ", "5"}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"3", "h"}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=3\n\[Therefore] strictly positive\n\nf(x) = -2x + 5\n\[CapitalDelta]f(x) = \
",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"f", "(", 
      RowBox[{
       SubscriptBox["x", "0"], "+", "h"}], ")"}], " ", "-", " ", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], 
    RowBox[{
     SubscriptBox["x", "0"], "+", "h", " ", "-", " ", 
     SubscriptBox["x", "0"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{
      RowBox[{"-", "2"}], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", "0"], "+", " ", "h"}], ")"}]}], " ", "+", " ", "5", 
     " ", "-", " ", 
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "2"}], 
        RowBox[{"(", 
         SubscriptBox["x", "0"], ")"}]}], " ", "+", " ", "5"}], "]"}]}], "h"],
    TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{
      RowBox[{"-", "2"}], 
      SubscriptBox["x", "0"]}], "-", 
     RowBox[{"2", "h"}], "+", "5", "+", 
     RowBox[{"2", 
      SubscriptBox["x", "0"]}], "-", "5"}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"-", "2"}], "h"}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=-2\n\[Therefore] strictly decreasing\n\nf(x) = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\[CapitalDelta]f(x) = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"f", "(", 
      RowBox[{
       SubscriptBox["x", "0"], "+", "h"}], ")"}], " ", "-", " ", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], 
    RowBox[{
     SubscriptBox["x", "0"], "+", "h", " ", "-", " ", 
     SubscriptBox["x", "0"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", "0"], "+", " ", "h"}], ")"}], "2"], "-", 
     SuperscriptBox[
      SubscriptBox["x", "0"], "2"]}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     SuperscriptBox[
      SubscriptBox["x", "0"], "2"], "+", 
     RowBox[{"2", "h", " ", 
      SubscriptBox["x", "0"]}], "+", 
     SuperscriptBox["h", "2"], " ", "-", 
     SuperscriptBox[
      SubscriptBox["x", "0"], "2"]}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"h", "(", 
     RowBox[{
      RowBox[{"2", " ", 
       SubscriptBox["x", "0"]}], "+", "h"}], ")"}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=2 ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "0"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " + h\n\[Therefore] Not strictly monotone\n\nf(x) = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "3"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\[CapitalDelta]f(x) = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"f", "(", 
      RowBox[{
       SubscriptBox["x", "0"], "+", "h"}], ")"}], " ", "-", " ", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], 
    RowBox[{
     SubscriptBox["x", "0"], "+", "h", " ", "-", " ", 
     SubscriptBox["x", "0"]}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", "0"], "+", " ", "h"}], ")"}], "3"], "-", 
     SuperscriptBox[
      SubscriptBox["x", "0"], "3"]}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["x", "0"], "2"], "+", 
        RowBox[{"2", "h", " ", 
         SubscriptBox["x", "0"]}], "+", 
        SuperscriptBox["h", "2"]}], " ", ")"}], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", "0"], "+", "h"}], ")"}]}], "-", 
     SuperscriptBox[
      SubscriptBox["x", "0"], "2"]}], "h"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     SuperscriptBox[
      SubscriptBox["x", "0"], "3"], "+", 
     RowBox[{"3", "h", " ", 
      SuperscriptBox[
       SubscriptBox["x", "0"], "2"]}], "+", 
     RowBox[{"3", 
      SuperscriptBox["h", "2"], " ", 
      SubscriptBox["x", "0"]}], "+", 
     SuperscriptBox["h", "3"], " ", "-", 
     SuperscriptBox[
      SubscriptBox["x", "0"], "3"]}], "h"], TraditionalForm]]],
 "=",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"3", " ", 
     SuperscriptBox[
      SubscriptBox["x", "0"], "2"]}], "+", 
    RowBox[{"3", "h", " ", 
     SubscriptBox["x", "0"]}], "+", 
    SuperscriptBox["h", "2"]}], TraditionalForm]]],
 "\n\[Therefore] Not strictly monotone"
}], "Text",
 CellChangeTimes->{{3.657475840192156*^9, 3.6574758494199314`*^9}, {
  3.6574758864597096`*^9, 3.6574765931978765`*^9}, {3.6574766333882837`*^9, 
