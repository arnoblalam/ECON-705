Cell[TextData[{
 "ABC = (AB)C\nFor the matrix AB the element ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["e", "rc"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " in the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["r", "th"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " row and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["c", "th"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " column is given as\n",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "r"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["B", "c", "T"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "  where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "r"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["r", "th"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " row of matrix A and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["B", "c"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["c", "th"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " column of matrix B.\n\nThen for ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["e", 
    RowBox[{"2", ",", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " we have [3, 4] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", GridBox[{
      {"5"},
      {"7"}
     }], "]"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 15 + 28 = 43\nSimilarly for ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["e", 
    RowBox[{"2", ",", " ", "2"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " we have [3, 4] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", GridBox[{
      {"6"},
      {"8"}
     }], "]"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 50\n\nFor the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["e", 
    RowBox[{"2", ",", " ", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " element of the matrix ABC, we will have\n",
 Cell[BoxData[
  FormBox[
   SubscriptBox["AB", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["C", "1"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = [43, 50] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", GridBox[{
      {"9"},
      {"11"}
     }], "]"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 937"
}], "Text",
