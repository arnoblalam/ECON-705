Cell[CellGroupData[{

Cell["Exercise 9", "Section",
 CellChangeTimes->{{3.65796012109155*^9, 3.6579601232382984`*^9}}],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"I", "-", "A"}], ")"}], "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = (I-A) (I-A)\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["I", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " - IA - AI + ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " (matrix multiplication is distributive)\nbut IA = AI = A (mutiplying by \
the identity is commutative)\nand ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["I", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = I (identity matrix is idempotent)\n\[Therefore] I - A - A + ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nbut A is idempotent so ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = A\n\[Therefore] ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"I", " ", "-", " ", "A"}], ")"}], "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = I - A\n\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = AA\n I - ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " =  (I - A) (I - A) = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"I", "-", "A"}], ")"}], "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n but ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"I", "-", "A"}], ")"}], "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"I", "-", "A"}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n \[Therefore] ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = A"
}], "Text",
 CellChangeTimes->{{3.6579601895968313`*^9, 3.657960257696237*^9}, {
   3.6579602879867864`*^9, 3.6579606338477325`*^9}, 3.6579608143342705`*^9, {
   3.657960862380021*^9, 3.6579608641613693`*^9}, {3.657960954317896*^9, 
