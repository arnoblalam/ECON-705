Cell[CellGroupData[{

Cell["Exercise 7", "Section",
 CellChangeTimes->{{3.6560435913587513`*^9, 3.656043594059675*^9}, {
  3.65611521314513*^9, 3.656115214882125*^9}}],

Cell[TextData[{
 "Consider the geometric series ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Sum]", 
     RowBox[{"k", " ", "=", " ", "0"}], "\[Infinity]"], 
    SuperscriptBox["x", "k"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", |x| < 1\nThis series converges to:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Sum]", 
     RowBox[{"k", " ", "=", " ", "0"}], "\[Infinity]"], 
    SuperscriptBox["x", "k"]}], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", 
    RowBox[{"1", "-", "x"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nDifferentiating (term by term), we obtain:\n",
 Cell[BoxData[
  FormBox[
   FractionBox["\[DifferentialD]", 
    RowBox[{"\[DifferentialD]", "x"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Sum]", 
     RowBox[{"k", " ", "=", " ", "0"}], "\[Infinity]"], 
    SuperscriptBox["x", "k"]}], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[DifferentialD]", 
     FractionBox["1", 
      RowBox[{"1", "-", "x"}]]}], 
    RowBox[{"\[DifferentialD]", "x"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Sum]", 
     RowBox[{"k", " ", "=", " ", "0"}], "\[Infinity]"], 
    RowBox[{"k", " ", 
     SuperscriptBox["x", 
      RowBox[{"k", "-", "1"}]]}]}], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "-", "x"}], ")"}], "2"]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nMultiplying by x:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Sum]", 
     RowBox[{"k", " ", "=", " ", "0"}], "\[Infinity]"], 
    RowBox[{"k", " ", 
     SuperscriptBox["x", "k"]}]}], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox["x", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "-", "x"}], ")"}], "2"]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nNow consider the geometric series given by\nPr(X=k) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "-", "p"}], ")"}], "k"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nThe expected value is:\n",
 Cell[BoxData[
  FormBox[
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"k", " ", "=", " ", "1"}], "\[Infinity]"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " k ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", "-", "p"}], ")"}], "k"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "p = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"p", "(", 
     RowBox[{"1", "-", "p"}], ")"}], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"(", 
        RowBox[{"1", "-", "p"}], ")"}]}], ")"}], "2"]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "= ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"p", "(", 
     RowBox[{"1", "-", "p"}], ")"}], 
    SuperscriptBox["p", "2"]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"1", "-", "p"}], "p"], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
