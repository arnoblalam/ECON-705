Cell["Exercise 11", "Section",
 CellChangeTimes->{{3.656043611988429*^9, 3.656043614238036*^9}}],

Cell[TextData[{
 "Consider the present value of the cash stream if the buyout is accepted:\n\
15000 - ",
 Cell[BoxData[
  FormBox[
   RowBox[{"200", " ", 
    RowBox[{"(", 
     FractionBox[
      RowBox[{"1", "-", " ", 
       SuperscriptBox[
        RowBox[{"(", "1.01", ")"}], 
        RowBox[{"-", "60"}]]}], ".01"], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " = 15000 - 8991.01 = 6009.00\nConsider the present value of the \
alternative:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"200", " ", 
    RowBox[{"(", 
     FractionBox[
      RowBox[{"1", "-", " ", 
       SuperscriptBox[
        RowBox[{"(", "1.01", ")"}], 
        RowBox[{"-", "60"}]]}], ".01"], ")"}]}], TraditionalForm]]],
 " = 8991.01\nNot accepting the buyout offer is better."
}], "Text",
 CellChangeTimes->{{3.65611996341215*^9, 3.6561200346779566`*^9}, {
  3.6561201895214972`*^9, 3.656120291404348*^9}, {3.6561204510380464`*^9, 
