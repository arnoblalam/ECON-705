       RowBox[{"e", " ", "=", " ", "eps"}], ",", " ", 
       RowBox[{"times", " ", "=", " ", "0"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"result", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{"True", ",", 
        RowBox[{"(", "\[IndentingNewLine]", 
