Cell[CellGroupData[{

Cell["Computational Exercise 1", "Section",
 CellChangeTimes->{{3.6518979246739807`*^9, 3.651897937697771*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"ClearAll", "[", 
  RowBox[{
  "lst", ",", " ", "x", ",", " ", "l", ",", " ", "square", ",", " ", 
   "newList"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"lst", " ", "=", " ", 
  RowBox[{"Range", "[", 
   RowBox[{"0", ",", " ", "15"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"newList", " ", "=", " ", 
