Cell[TextData[{
 "Maximize ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "0.5"], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["y", "0.5"], TraditionalForm]]],
 " s.t. 2 x + y = k\n\n\[ScriptCapitalL] (x, y, \[Lambda]) = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "0.5"], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["y", "0.5"], TraditionalForm]]],
 " + \[Lambda] (2 x + y - k)\n\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
    RowBox[{"\[PartialD]", "x"}]], TraditionalForm]]],
 " = 0.5 ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", 
    RowBox[{"-", ".05"}]], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["y", "0.5"], TraditionalForm]]],
 " + 2 \[Lambda] = 0\n\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
    RowBox[{"\[PartialD]", "x"}]], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    FractionBox["1", 
     RowBox[{"2", " ", 
      RowBox[{"\[Sqrt]", "x"}]}]], ")"}], TraditionalForm]]],
 " \[Sqrt] y + 2 \[Lambda] = 0\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
    RowBox[{"\[PartialD]", "y"}]], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "0.5"], TraditionalForm]]],
 " (0.5 ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["y", 
    RowBox[{"-", "0.5"}]], TraditionalForm]]],
 ") + \[Lambda] = 0\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
    RowBox[{"\[PartialD]", "\[Lambda]"}]], TraditionalForm]]],
 " = 2 x + y - k = 0\n\n\[Lambda] = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "2"], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[Sqrt]", "x"}], 
    RowBox[{"\[Sqrt]", "y"}]], TraditionalForm]]],
 "\n2 \[Lambda] =  ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[Sqrt]", "x"}], 
    RowBox[{"\[Sqrt]", "y"}]], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[PartialD]", "\[ScriptCapitalL]"}], 
    RowBox[{"\[PartialD]", "x"}]], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    FractionBox[
     RowBox[{"\[Sqrt]", "y"}], 
     RowBox[{"2", " ", 
      RowBox[{"\[Sqrt]", "x"}]}]], ")"}], TraditionalForm]]],
 "  = - ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[Sqrt]", "x"}], 
    RowBox[{"\[Sqrt]", "y"}]], TraditionalForm]]],
 "\ny = 2 x - k\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    FractionBox["1", 
     RowBox[{"2", " ", 
      RowBox[{"\[Sqrt]", "x"}]}]], ")"}], TraditionalForm]]],
 " \[Sqrt](2 x - k) + ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[Sqrt]", "x"}], 
    RowBox[{"\[Sqrt]", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "x"}], "-", "k"}], ")"}]}]], TraditionalForm]]],
 " = 0\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    FractionBox["1", 
     RowBox[{"2", " ", 
      RowBox[{"\[Sqrt]", "x"}]}]], ")"}], TraditionalForm]]],
 " \[Sqrt](2 x - k) = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"-", 
     RowBox[{"\[Sqrt]", "x"}]}], 
    RowBox[{"\[Sqrt]", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "x"}], "-", "k"}], ")"}]}]], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    FractionBox["1", 
     RowBox[{"2", " ", 
      RowBox[{"\[Sqrt]", "x"}]}]], ")"}], TraditionalForm]]],
 " (2 x - k) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    RowBox[{"\[Sqrt]", "x"}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", 
     RowBox[{"4", "x"}]], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", " ", "x"}], " ", "-", " ", "k"}], ")"}], "2"]}], 
   TraditionalForm]]],
 "= ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", 
     RowBox[{"4", "x"}]], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", " ", "x"}], " ", "-", " ", "k"}], ")"}]}], 
   TraditionalForm]]],
 "(2 x - k) = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", 
     RowBox[{"4", "x"}]], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"4", " ", 
       SuperscriptBox["x", "2"]}], " ", "-", " ", 
      RowBox[{"4", "kx"}], " ", "+", " ", 
      SuperscriptBox["k", "2"]}], ")"}]}], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]]],
 "\nx - k + ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    SuperscriptBox["k", "2"], 
    RowBox[{"4", "x"}]], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]]],
 " - k x + ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    SuperscriptBox["k", "2"], "4"], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "3"], TraditionalForm]]],
 "\n-",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "3"], TraditionalForm]]],
 "+ ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]]],
 " - k x + ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    SuperscriptBox["k", "2"], "4"], TraditionalForm]]],
 " = 0"
}], "Text",
